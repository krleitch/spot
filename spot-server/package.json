  "_moduleAliases": {
    "@models": "../spot-commons/build/models",
    "@exceptions": "../spot-commons/build/exceptions",
    "@constants": "../spot-commons/build/constants",
    "@db": "src/app/db",
    "@routes": "src/app/routes",
    "@services": "src/app/services",
    "@helpers": "src/app/helpers",
    "@config": "src/config"
  },
  "scripts": {
    "clean": "rimraf build",
    "compile": "cross-env-shell babel src -d build --source-maps --extensions '.ts'",
    "build": "npm run clean && npm run compile",
    "typecheck": "tsc --p .",
    "build-typecheck": "npm run typecheck && npm run build",
    "start": "npm run build-typecheck && node ./build/app/main.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "dev": "concurrently \"tsc -w\" \"node-dev --respawn build/app/main.js\"",
    "prod": "tsc && node ./build/main.js",
    "lint": "npx eslint 'src/**/*.{js,ts}' --quiet --fix",
    "format": "npx prettier 'src/**/*.{js,ts}' --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krleitch/spot-server.git"

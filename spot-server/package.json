    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "email-templates": "^8.0.8",
    "express": "^4.16.4",
    "express-rate-limit": "^5.1.3",
    "google-auth-library": "^6.0.6",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
